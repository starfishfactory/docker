# PostgreSQL 설정
POSTGRES_USER=n8n
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=n8n

# n8n 기본 인증 설정
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your_admin_password_here

# n8n 호스트 설정
# NAS의 실제 IP 주소나 도메인으로 변경하세요
N8N_HOST=192.168.1.100
N8N_PROTOCOL=http
WEBHOOK_URL=http://192.168.1.100:5678/

# 타임존 설정
GENERIC_TIMEZONE=Asia/Seoul

# 암호화 키 (32자 이상의 랜덤 문자열로 변경하세요!)
# 예: openssl rand -hex 32
N8N_ENCRYPTION_KEY=your_random_encryption_key_here